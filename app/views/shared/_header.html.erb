<% if @current_user %>
  <div id='welcome'>
  	Welcome <%= @current_user.name %>
  	<div class="right"><%= link_to "Sign out", '/signout' %></div>
  </div>
  <ul>
    <li style="list-style: none;"><%= link_to "Tweet", messages_path %></li> 
  </ul>
<% else %>
  <%= link_to "Sign in with Twitter", "/auth/twitter" %> 
<% end %>